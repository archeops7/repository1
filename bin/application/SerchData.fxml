<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SerchDataController">
   <top>
      <AnchorPane prefHeight="400.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableSerchData" layoutX="15.0" layoutY="40.0" prefHeight="320.0" prefWidth="770.0">
              <columns>
                <TableColumn fx:id="km" prefWidth="68.0" text="走行距離" />
                <TableColumn fx:id="totalKm" prefWidth="68.0" text="累計距離" />
                  <TableColumn fx:id="litter" prefWidth="68.0" text="消費燃料量" />
                  <TableColumn fx:id="totalLitter" prefWidth="68.0" text="累計燃料量" />
                  <TableColumn fx:id="mileage" prefWidth="68.0" text="今回燃費" />
                  <TableColumn fx:id="aveMileage" prefWidth="68.0" text="平均燃費" />
                  <TableColumn fx:id="price" prefWidth="68.0" text="ガソリン単価" />
                  <TableColumn fx:id="cost" prefWidth="68.0" text="今回燃料費" />
                  <TableColumn fx:id="totalCost" prefWidth="68.0" text="累計燃料費" />
                  <TableColumn fx:id="memo" prefWidth="87.0" text="メモ" />
                  <TableColumn fx:id="date" prefWidth="68.0" text="日付" />
              </columns>
            </TableView>
            <Button fx:id="buttonSerch" layoutX="733.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonClickedSerch" text="検索" />
            <TextField fx:id="fieldWord" layoutX="505.0" layoutY="14.0" prefHeight="25.0" prefWidth="213.0" />
            <Label layoutX="381.0" layoutY="18.0" text="メモのキーワードから検索：" />
            <Button fx:id="buttonReturn" layoutX="734.0" layoutY="361.0" mnemonicParsing="false" onAction="#buttonClickedReturn" text="戻る" />
         </children>
      </AnchorPane>
   </top>
</BorderPane>
